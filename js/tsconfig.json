  "compilerOptions": {
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "module": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "es5",
    "allowJs": true,
    "noEmit": true
  },
  "include": [
    "./src/**/*"
  ]
}
